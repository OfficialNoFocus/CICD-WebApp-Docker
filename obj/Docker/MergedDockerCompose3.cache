services:
  webapi:
    build:
      context: C:\Users\Mick van der Woude\source\repos\CICD WebApp
      dockerfile: WebApi/Dockerfile
      labels:
        com.microsoft.created-by: visual-studio
        com.microsoft.visual-studio.project-name: WebApi
      target: base
    container_name: WebApi
    entrypoint: tail -f /dev/null
    environment:
      ASPNETCORE_ENVIRONMENT: Development
      ASPNETCORE_LOGGING__CONSOLE__DISABLECOLORS: "true"
      ASPNETCORE_URLS: https://+:443;http://+:80
      DOTNET_USE_POLLING_FILE_WATCHER: '1'
      NUGET_FALLBACK_PACKAGES: /root/.nuget/fallbackpackages
    image: webapi:dev
    labels:
      com.microsoft.visualstudio.debuggee.arguments: ' --additionalProbingPath /root/.nuget/packages
        --additionalProbingPath /root/.nuget/fallbackpackages  "/app/bin/Debug/net5.0/WebApi.dll"'
      com.microsoft.visualstudio.debuggee.killprogram: /bin/sh -c "if PID=$$(pidof
        dotnet); then kill $$PID; fi"
      com.microsoft.visualstudio.debuggee.program: dotnet
      com.microsoft.visualstudio.debuggee.workingdirectory: /app
    ports:
    - published: 51080
      target: 80
    - published: 51443
      target: 443
    tty: true
    volumes:
    - C:\Users\Mick van der Woude\source\repos\CICD WebApp\WebApi:/app:rw
    - C:\Users\Mick van der Woude\vsdbg\vs2017u5:/remote_debugger:rw
    - C:\Users\Mick van der Woude\AppData\Roaming\ASP.NET\Https:/root/.aspnet/https:ro
    - C:\Users\Mick van der Woude\AppData\Roaming\Microsoft\UserSecrets:/root/.microsoft/usersecrets:ro
    - C:\Program Files\dotnet\sdk\NuGetFallbackFolder:/root/.nuget/fallbackpackages:ro
    - C:\Users\Mick van der Woude\.nuget\packages:/root/.nuget/packages:ro
    - C:\Users\Mick van der Woude\source\repos\CICD WebApp:/src:rw
  webapp:
    build:
      context: C:\Users\Mick van der Woude\source\repos\CICD WebApp
      dockerfile: WebApp/Dockerfile
      labels:
        com.microsoft.created-by: visual-studio
        com.microsoft.visual-studio.project-name: WebApp
      target: base
    container_name: WebApp
    entrypoint: tail -f /dev/null
    environment:
      ASPNETCORE_ENVIRONMENT: Development
      ASPNETCORE_LOGGING__CONSOLE__DISABLECOLORS: "true"
      ASPNETCORE_URLS: https://+:443;http://+:80
      DOTNET_USE_POLLING_FILE_WATCHER: '1'
      NUGET_FALLBACK_PACKAGES: /root/.nuget/fallbackpackages
      WebApiBaseAddress: http://webapi
    image: webapp:dev
    labels:
      com.microsoft.visualstudio.debuggee.arguments: ' --additionalProbingPath /root/.nuget/packages
        --additionalProbingPath /root/.nuget/fallbackpackages  "/app/bin/Debug/net5.0/WebApp.dll"'
      com.microsoft.visualstudio.debuggee.killprogram: /bin/sh -c "if PID=$$(pidof
        dotnet); then kill $$PID; fi"
      com.microsoft.visualstudio.debuggee.program: dotnet
      com.microsoft.visualstudio.debuggee.workingdirectory: /app
    ports:
    - published: 50080
      target: 80
    - published: 50443
      target: 443
    tty: true
    volumes:
    - C:\Users\Mick van der Woude\source\repos\CICD WebApp\WebApp:/app:rw
    - C:\Users\Mick van der Woude\vsdbg\vs2017u5:/remote_debugger:rw
    - C:\Users\Mick van der Woude\AppData\Roaming\ASP.NET\Https:/root/.aspnet/https:ro
    - C:\Users\Mick van der Woude\AppData\Roaming\Microsoft\UserSecrets:/root/.microsoft/usersecrets:ro
    - C:\Program Files\dotnet\sdk\NuGetFallbackFolder:/root/.nuget/fallbackpackages:ro
    - C:\Users\Mick van der Woude\.nuget\packages:/root/.nuget/packages:ro
    - C:\Users\Mick van der Woude\source\repos\CICD WebApp:/src:rw
version: '3.4'